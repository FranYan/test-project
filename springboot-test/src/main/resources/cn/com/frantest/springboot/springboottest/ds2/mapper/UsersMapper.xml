<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.com.frantest.springboot.springboottest.ds2.mapper.UsersMapper">

    <select id="findAll" resultType="cn.com.frantest.springboot.springboottest.ds2.entity.Users">
        select * from user;
    </select>

    <insert id="insertUsers" parameterType="cn.com.frantest.springboot.springboottest.ds2.entity.Users">
        insert into user(id, username) values (#{id}, #{username});
    </insert>

</mapper>